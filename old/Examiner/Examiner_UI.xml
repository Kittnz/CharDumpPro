									</Anchors>
								</FontString>
							</Layer>
						</Layers>
						<Frames>

							<!-- Player Entries (8) -->
							<Button name="$parentEntry1" inherits="ExaminerCacheEntryTemplate" id="1">
								<Anchors>
									<Anchor point="TOPLEFT">
										<Offset x="8" y="-40"/>
