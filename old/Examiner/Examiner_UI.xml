								<Layers>
									<Layer level="ARTWORK">
										<FontString name="$parentCached" inherits="ExaminerHeaderFontTemplate" text="I">
											<Color r="0.75" g="0.75" b="0.75"/>
										</FontString>
									</Layer>
								</Layers>
								<Scripts>
									<OnEnter>
										Examiner_InfoIcon_OnEnter(self);
									</OnEnter>
									<OnLeave>
										GameTooltip:Hide();
									</OnLeave>
								</Scripts>
							</Frame>

							<!-- Cached Entry Icon -->
