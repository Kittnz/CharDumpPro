						<Anchor point="TOP" relativeTo="$parentDetails" relativePoint="BOTTOM">
							<Offset x="0" y="-2"/>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
			<Layer level="OVERLAY">

