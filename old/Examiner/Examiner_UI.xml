					<!-- PVP Frame -->
					<Frame name="Examiner_PVPFrame" inherits="ExaminerBaseFrame">
					 	<Size x="320" y="330"/>
						<Layers>
							<Layer level="ARTWORK">

								<FontString name="$parentHeader" inherits="ExaminerHeaderFontTemplate">
