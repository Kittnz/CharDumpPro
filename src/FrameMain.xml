<Ui xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.blizzard.com/wow/ui/">
	<Script file="FrameMain.lua" />
	<Frame name="frameMain" hidden="true" movable="true">
		<Size>
			<AbsDimension x="510" y="422" />
		</Size>
		<Anchors>
			<Anchor point="CENTER">
				<Offset x="-5" y="46" />
			</Anchor>
		</Anchors>
		<TitleRegion setAllPoints="true">
			<Size>
				<AbsDimension x="0" y="30" />
			</Size>
			<Anchors>
				<Anchor point="TOPLEFT" />
				<Anchor point="TOPRIGHT" />
			</Anchors>
		</TitleRegion>
		<Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
			<BackgroundInsets>
				<AbsInset left="11" right="12" top="12" bottom="11" />
			</BackgroundInsets>
			<TileSize>
				<AbsValue val="32" />
			</TileSize>
			<EdgeSize>
				<AbsValue val="32" />
			</EdgeSize>
		</Backdrop>
		<Layers>
			<Layer>
				<Texture file="Interface\DialogFrame\UI-DialogBox-Corner">
					<Size>
						<AbsDimension x="40" y="40" />
					</Size>
					<Anchors>
						<Anchor point="TOPRIGHT">
							<Offset x="-4" y="-5" />
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Frames>
			<Button name="btnDump" inherits="UIPanelButtonTemplate" text="Dump Character">
				<Size>
					<AbsDimension x="133" y="33" />
				</Size>
				<Anchors>
					<Anchor point="BOTTOM" relativeTo="frameMain">
						<Offset x="40" y="302" />
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						btnDump_OnClick()
					</OnClick>
				</Scripts>
				<ButtonText name="$parentText" font="Fonts\FRIZQT__.TTF" text="" />
			</Button>
			<Button name="btnClose" inherits="UIPanelCloseButton" text="Button1">
				<Size>
					<AbsDimension x="40" y="40" />
				</Size>
				<Anchors>
					<Anchor point="TOPRIGHT" relativeTo="frameMain">
						<Offset x="1" />
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						btnClose_OnClick()
					</OnClick>
				</Scripts>
			</Button>
			<Frame name="header">
				<Size>
					<AbsDimension x="294" y="66" />
				</Size>
				<Anchors>
					<Anchor point="TOP">
						<Offset y="12" />
					</Anchor>
				</Anchors>
				<Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Header" edgeFile="">
					<BackgroundInsets>
						<AbsInset xsi:nil="true" />
					</BackgroundInsets>
				</Backdrop>
				<Layers>
					<Layer level="OVERLAY">
						<FontString name="headerText" inherits="GameFontNormal" text="CD-Pro v1.1">
							<Size>
								<AbsDimension x="131" y="20" />
							</Size>
							<Anchors>
								<Anchor point="CENTER">
									<Offset x="-2" y="12" />
								</Anchor>
							</Anchors>
							<FontHeight>
								<AbsValue val="15" />
							</FontHeight>
						</FontString>
					</Layer>
				</Layers>
			</Frame>
		</Frames>
		<Scripts>
			<OnLoad>
				frameMain_OnLoad()
			</OnLoad>
			<OnDragStart>
				self:StartMoving();
			</OnDragStart>
			<OnDragStop>
				self:StopMovingOrSizing();
			</OnDragStop>
		</Scripts>
	</Frame>
</Ui>